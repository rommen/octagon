<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ArtistSquare\ArtistBundle\Entity\Article" table="Article">
    <indexes>
      <index name="fk_Article_category1_idx" columns="category"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="true"/>
    <field name="text" type="text" column="text" nullable="true"/>
    <many-to-one field="category" target-entity="Category">
      <join-columns>
        <join-column name="category" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="tag" target-entity="Tag" mapped-by="article"/>
  </entity>
</doctrine-mapping>
