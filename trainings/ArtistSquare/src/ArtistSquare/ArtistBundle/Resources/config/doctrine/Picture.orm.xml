<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ArtistSquare\ArtistBundle\Entity\Picture" table="Picture">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="100" nullable="false"/>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="blockReason" type="text" column="block_reason" nullable="true"/>
    <many-to-many field="tag" target-entity="Tag" mapped-by="picture"/>
    <many-to-many field="gallery" target-entity="Gallery" mapped-by="picture"/>
  </entity>
</doctrine-mapping>
