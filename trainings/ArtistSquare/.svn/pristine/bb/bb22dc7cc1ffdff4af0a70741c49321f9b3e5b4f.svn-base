.gallery_fence{
    width: 170px;
    height: 220px;
    
    background-color: red;
}

.fence_image img{
    width:170px;
}

.fence_rate{
    height: 22px;
    position: relative;
    top: -23px;
    background-color: rgba(245, 245, 245, 0.7);
    color: #192E41;
}

div.fence_rate div{
    margin-left:4px;
    margin-right:4px;
    padding-top:3px;
    height:22px;
}
.fence_image{
    height:66.67%;
}

.fence_text{
    position: relative;
    top: 23px;
    
    width:100%;
    height:50px;
    
    color: white;
    background-color:gray;
}