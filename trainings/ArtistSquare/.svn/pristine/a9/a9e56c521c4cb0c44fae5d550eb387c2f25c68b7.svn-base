<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ArtistSquare\ArtistBundle\Entity\Category" table="Category">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="title" type="string" column="title" length="45" nullable="false"/>
    <field name="description" type="string" column="description" length="250" nullable="false"/>
  </entity>
</doctrine-mapping>
